						script.
							var CJSMuzMapFilter = {
							clear : function () {
							$('.js_projects_map__search__form__input').val('');
							$('.js_projects_map__search__form__list').hide();
							$('.js_projects_map__search__form__list').html('');
							$('.js_projects_map__search__form__clear').hide();
							},
							setAll : function (propId, isAll) {
							if (isAll) {
							$('.js_map_filter_prop_'+propId).removeAttr('disabled').removeAttr('checked').prop('checked', false);
							} else {
							$('.js_map_filter_prop_'+propId+'__allvalue').removeAttr('checked').prop('checked', false);
							}
							},
							submitFilter : function () {
							let obForm = $('#js_map_smart_filter');
							$.ajax({
							url : '/map/',
							type : 'GET',
							data : obForm.serialize()+'&setAjaxFilter=Y',
							success : function (data) {
							$('#js_map_list_result').html('').append(data);
							}
							});
							},
							calcSelectedItems : function () {
							$('.js_map_filter_props_all').each(function () {
							let propId = $(this).data('prop-id');
							let countSelected = 0;
							$('.js_map_filter_prop_'+propId).each(function () {
							if ($(this).is(":checked")) {
							countSelected++;
							}
							});
							$('.js_map_filter_props_'+propId+'__count').html(countSelected);
							if (countSelected > 0) {
							$('.js_map_filter_props_'+propId+'__selected').show();
							$('.js_map_filter_props_'+propId+'__count').show();
							$('.js_map_filter_props_'+propId+'__all').hide();
							} else {
							$('.js_map_filter_props_'+propId+'__selected').hide();
							$('.js_map_filter_props_'+propId+'__count').hide();
							$('.js_map_filter_props_'+propId+'__all').show();
							}
							});
							},
							searchData : function (obInput) {
							let obForm = $('#js_muz_search_data');
							if (obInput.val().length <= 0) {
							$('.js_projects_map__search__form__list').hide();
							$('.js_projects_map__search__form__clear').hide();
							} else {
							$('.js_projects_map__search__form__clear').show();
							}
							$.ajax({
							url : obForm.attr('action'),
							type : 'POST',
							data : 'ACTION=SEARCH&Q='+obInput.val(),
							success : function (data) {
							if (data.empty) {
							$('.js_projects_map__search__form__list').hide();
							$('.js_projects_map__search__form__list').html('');
							} else {
							$('.js_projects_map__search__form__list').show();
							$('.js_projects_map__search__form__list').html('');
							$('.js_projects_map__search__form__list').append(data.html);
							}
							}
							});
							}
							};
							$(document).ready(function () {
							$('.js_projects_map_menu').click(function () {
							$('.js_projects_map_menu_block').toggleClass('stage_open');
							});
							$('.projects_map__select__inner').click(function () {
							//$('.projects_map__select__list.open-stage').not($(this)).removeClass('open-stage');
							$(this).siblings('.projects_map__select__list').toggleClass('open-stage');
							return false;
							});
							$('#js_map_list_result').click(function () {
							$('.projects_map__select__list').removeClass('open-stage');
